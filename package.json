{
  "name": "distributed-jwt-cracker",
  "version": "0.0.2",
  "description": "An experimental distributed JWT token cracker built using Node.js and ZeroMQ",
  "bin": {
    "jwt-cracker-server": "src/server.js",
    "jwt-cracker-client": "src/client.js"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "node_modules/.bin/tap test/ --coverage | node_modules/.bin/tap-spec",
    "posttest": "node_modules/.bin/tap --coverage-report=lcov && node_modules/.bin/codecov"
  },
  "author": {
    "name": "Luciano Mammino",
    "email": "lucianomammino@gmail.com",
    "url": "http://loige.co"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "dateformat": "^1.0.12",
    "indexed-string-variation": "^1.0.1",
    "jsonwebtoken": "^7.1.9",
    "winston": "^2.2.0",
    "yargs": "^5.0.0",
    "zmq": "^2.15.3"
  },
  "keywords": [
    "jwt",
    "token",
    "cracker",
    "brute force",
    "password",
    "brute force",
    "HS256",
    "secret",
    "break"
  ],
  "devDependencies": {
    "codecov": "^1.0.1",
    "mockery": "^1.7.0",
    "tap": "^7.1.2",
    "tap-spec": "^4.1.1"
  }
}
